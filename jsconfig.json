{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@reducers/*": [
        "./src/reducers/*"
      ],
      "@actions/*": [
        "./src/actions/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@base/*": [
        "./public/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@middleware/*": [
        "./src/middleware/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}